		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.3.10",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.18",
		"directus-sdk-typegen": "^0.1.11",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"formsnap": "^2.0.0",
